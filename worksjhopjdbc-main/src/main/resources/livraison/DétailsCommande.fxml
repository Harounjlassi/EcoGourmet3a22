<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tn.esprit.controllers.livraison.DetailsCommande">
    <children>
        <TableView fx:id="tableView" prefHeight="500.0" prefWidth="500.0" style="-fx-background-color: #f8f8f8; -fx-border-color: #e8e8e8; -fx-border-width: 1px;">
            <columns>
                <TableColumn fx:id="nomDuPlatColumn" text="Nom du plat" prefWidth="125.0" />
                <TableColumn fx:id="quantiteColumn" text="Quantite" prefWidth="125.0" />
                <TableColumn fx:id="prixTotalColumn" text="Prix Total" prefWidth="125.0" />
                <TableColumn fx:id="adresseColumn" text="Adresse" prefWidth="125.0" />
            </columns>
        </TableView>
    </children>
</AnchorPane>